# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  AdventOfCode2023_config = debug

else ifeq ($(config),release)
  AdventOfCode2023_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := AdventOfCode2023

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

AdventOfCode2023:
ifneq (,$(AdventOfCode2023_config))
	@echo "==== Building AdventOfCode2023 ($(AdventOfCode2023_config)) ===="
	@${MAKE} --no-print-directory -C . -f AdventOfCode2023.make config=$(AdventOfCode2023_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f AdventOfCode2023.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   AdventOfCode2023"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"